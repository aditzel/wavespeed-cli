{
  "name": "wavespeed-cli",
  "version": "1.1.0",
  "description": "CLI for Wavespeed AI Bytedance Seedream V4 API",
  "type": "module",
  "bin": {
    "wavespeed": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aditzel/wavespeed-cli.git"
  },
  "homepage": "https://github.com/aditzel/wavespeed-cli#readme",
  "bugs": {
    "url": "https://github.com/aditzel/wavespeed-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "wavespeed",
    "ai",
    "image-generation",
    "video-generation",
    "cli",
    "seedream"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm && chmod +x dist/index.js",
    "dev": "bun run --hot src/index.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "lint": "biome check src tests",
    "lint:fix": "biome check --write src tests",
    "format": "biome format src tests"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "commander": "^14.0.1",
    "prompts": "^2.4.2",
    "yaml": "^2.8.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/bun": "latest",
    "@types/node": "^24.8.1",
    "@types/prompts": "^2.4.9",
    "bun-types": "^1.3.0",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3"
  },
  "license": "MIT"
}
